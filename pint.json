{
	"preset": "laravel",
	"rules": {
		"no_unused_imports": true,
		"ordered_imports": {
			"sort_algorithm": "alpha"
		},
		"phpdoc_align": true,
		"phpdoc_order": true,
		"phpdoc_no_empty_return": true
	},
	"exclude": [
		"vendor",
		"storage",
		"bootstrap/cache",
		"node_modules"
	],
	"paths": [
		"app",
		"src",
		"routes",
		"config",
		"database",
		"tests"
	]
}

